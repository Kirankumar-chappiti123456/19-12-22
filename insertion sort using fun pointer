#include<stdio.h>
int fun(int arr[],int len)
{
    
   
    int i=0,j=0,temp;
    for(i=1;i<len;i++)
    {
       temp=arr[i];
       j=i-1;
       while(j>=0 && arr[j]>temp)
       {
           arr[j+1]=arr[j];
           j--;
       }
       arr[j+1]=temp;
    }
    for(i=0;i<len;i++)
    {
        printf("%d ",arr[i]);
    }
}
int main()
{
    int arr[]={44,11,1,4,8,3,9,19,22};
     int len=sizeof(arr)/sizeof(arr[0]);
    int (*fptr)(int [],int )=fun;
    fptr(arr,len);
}
